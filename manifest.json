{
  "manifest_version": 3,
  "name": "Autocompletado Rama Judicial",
  "version": "1.0",
  "description": "Extensi√≥n para autocompletar formularios de la Rama Judicial",

  "permissions": [
    "activeTab",
    "storage"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_title": "Autocompletado Rama Judicial"
  },

  "content_scripts": [
    {
      "matches": ["*://procesojudicial.ramajudicial.gov.co/*"],
      "js": ["content.js"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ],

  "icons": {
     "16": "icons/icon16.png",
     "48": "icons/icon48.png",
     "128": "icons/icon128.png"
  }
}
